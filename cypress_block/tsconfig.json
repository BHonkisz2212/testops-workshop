{
    "compilerOptions": {
        "importHelpers": true,
        "baseUrl": ".",
        "allowSyntheticDefaultImports": true,
        "moduleResolution": "Node",
        "strict": true,
        "target": "es2019",
        "lib": [
            "es2019",
            "DOM"
        ],
        "types": [
            "cypress","node","cypress-plugin-snapshots"
        ],
        "allowJs": true,
        "noEmit": true,
    },
    "files": [
        "cypress/support/index.d.ts"
    ],
    "include": [
        "node_modules/cypress",
        "cypress/**/*.ts",
        "cypress/**/*.js",
        "**/*.ts"
    ]
}